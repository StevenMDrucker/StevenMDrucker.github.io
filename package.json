{
  "name": "sdruckerhome-app",
  "private": true,
  "version": "1.0.0",
  "homepage": "https://stevenmdrucker.github.io/SDruckerHome",
  "scripts": {
    "predeploy": "rimraf ./dist && parcel build ./docs/src/index.htm --public-url ./dist --out-dir ./dist && copy .\\dist\\index.html .\\index.html",
    "deploy": "git add -A && git commit -a -m 'deploy' && git push",
    "oldpublish": "gh-pages -b 'master' -d ./dist",
    "publish": "git add -A && git commit -a -m 'deploy' && git push",
    "build": "rimraf ./dist &&  parcel build ./docs/src/index.htm --public-url ./dist --out-dir ./dist && copy .\\dist\\index.html .\\index.html",
    "buildtsc": "tsc -p .",
    "start": "parcel serve ./docs/src/index.htm --open",
    "copy": "copy .\\dist\\index.html .\\index.html"
  },
  "devDependencies": {
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "cssnano": "^4.1.10",
    "sass": "^1.23.0",
    "typescript": "^3.6.3"
  },
  "alias": {
    "react": "./alias/react-external.js",
    "react-dom": "./alias/react-dom-external.js"
  },
  "dependencies": {
    "@types/d3": "^5.7.2",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-slick": "^0.23.4",
    "d3": "^5.12.0",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-bootstrap": "^0.32.4",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.10.2",
    "react-html-parser": "^2.0.2",
    "react-markdown": "^7.0.1",
    "react-masonry-component": "^6.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.23.2",
    "react-stonecutter": "^0.3.10",
    "remark-gfm": "^2.0.0",
    "rimraf": "^3.0.2"
  }
}
